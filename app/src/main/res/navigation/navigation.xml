<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/frag5ShowListTimeslots">

    <fragment
        android:id="@+id/frag1ShowProfile"
        android:name="it.polito.group06.Frag1ShowProfile"
        android:label="Frag1ShowProfile" >
        <action
            android:id="@+id/action_frag1ShowProfile_to_frag2EditProfile"
            app:destination="@id/frag2EditProfile" />
    </fragment>
    <fragment
        android:id="@+id/frag2EditProfile"
        android:name="it.polito.group06.Frag2EditProfile"
        android:label="Frag2EditProfile" >
        <action
            android:id="@+id/action_frag2EditProfile_to_frag1ShowProfile"
            app:destination="@id/frag1ShowProfile" />
    </fragment>
    <fragment
        android:id="@+id/frag3ShowTimeslot"
        android:name="it.polito.group06.Frag3ShowTimeslot"
        android:label="Frag3ShowTimeslot" >
        <action
            android:id="@+id/action_frag3ShowTimeslot_to_frag4EditTimeslot"
            app:destination="@id/frag4EditTimeslot" />
    </fragment>
    <fragment
        android:id="@+id/frag4EditTimeslot"
        android:name="it.polito.group06.Frag4EditTimeslot"
        android:label="Frag4EditTimeslot" >
        <action
            android:id="@+id/action_frag4EditTimeslot_to_frag3ShowTimeslot"
            app:destination="@id/frag3ShowTimeslot" />
    </fragment>
    <fragment
        android:id="@+id/frag5ShowListTimeslots"
        android:name="it.polito.group06.Frag5ShowListTimeslots"
        android:label="Frag5ShowListTimeslots" >
        <action
            android:id="@+id/action_frag5ShowListTimeslots_to_frag3ShowTimeslot"
            app:destination="@id/frag3ShowTimeslot" />
        <action
            android:id="@+id/action_frag5ShowListTimeslots_to_frag1ShowProfile"
            app:destination="@id/frag1ShowProfile" />
    </fragment>
</navigation>